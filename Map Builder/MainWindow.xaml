<Window x:Class="Map_Builder.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Map_Builder"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800"
        KeyDown="KeyPressed"
        KeyUp="KeyReleased"
        MouseLeftButtonDown="LeftClick"
        MouseRightButtonDown="RightClick">
    <Grid>


        <Canvas x:Name="canvas">

        </Canvas>
        
        <StackPanel x:Name="stackpannel" Orientation="Horizontal">
            <ScrollViewer VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Disabled" FlowDirection="LeftToRight">
                <Grid x:Name="TemplateGrid" VerticalAlignment="Top" HorizontalAlignment="Left">
                </Grid>
            </ScrollViewer>
        </StackPanel>
    </Grid>
</Window>
